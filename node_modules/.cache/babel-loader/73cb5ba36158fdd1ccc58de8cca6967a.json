ome</div>\n      <!-- 花火のアニメーション -->\n      <lottie :options=\"fireworksLottie\" :height=\"300\" :width=\"300\" :animCreated=\"handleAnimation\"/>\n    </el-dialog>\n  </div>\n</template>\n\n<script>\nimport Lottie from \"@/components/Lottie.vue\"\nimport * as welcome from \"@/assets/welcome.json\"\n// 花火のアニメーション\nimport * as fireworks from \"@/assets/fireworks.json\"\n\nexport default {\n  name: 'app',\n  components: {\n    Lottie\n  },\n  data () {\n    return {\n      checking: false,\n      dialogTableVisible: false,\n      loginForm: {\n        email: '',\n        password: '',\n      }\n    }\n  },\n  computed: {\n    welcomeLottie () {\n      return { animationData: welcome }\n    },\n    fireworksLottie () {\n      return { animationData: fireworks }\n    }\n  },\n  methods: {\n    handleAnimation (anim) {\n      this.anim = anim\n    },\n    handleLogin() {\n      this.checking = true\n      // 演出のためです...。追加しました。\n      setTimeout(() => {\n        this.checking = false\n        this.dialogTableVisible = true\n      }, 1000)\n    }\n  }\n}\n</script>\n\n<style>\n  body {\n    text-align: center;\n    background-color: #E0EAF6;\n  }\n\n  .login-title {\n    font-size: 32px;\n    font-weight: 600;\n    margin-bottom: 20px;\n  }\n\n  .login-card {\n    max-width: 800px;\n    margin: 200px auto 0;\n    padding: 120px 0 20px;\n  }\n\n  .login-lottie {\n    position: absolute;\n    top: 32px;\n    right: 0;\n    bottom: 0;\n    left: 0;\n    margin: 0 auto;\n  }\n</style>\n"],"sourceRoot":"src"}]}